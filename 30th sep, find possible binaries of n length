class Solution {
    public ArrayList<String> binstr(int n) {
        // code here
        int nn= (int)Math.pow(2,n);
        ArrayList<String>res = new ArrayList<>(nn);
        
        for(int i=0;i<nn;i++){
            String s = Integer.toBinaryString(i);
            
            while(s.length()<n){
                s=0+s;
            }
            
            res.add(s);
        }
        return res;
    }
}


// find the binary possible values of given n length
