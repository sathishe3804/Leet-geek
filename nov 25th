class Solution {
    public int subarrayXor(int[] arr) {
        int n = arr.length;
        int ans = 0;
        
        for (int i = 0; i < n; i++) {
            long count = (long)(i + 1) * (n - i); // use long to avoid overflow
            
            if ((count & 1) == 1) { // odd
                ans ^= arr[i];
            }
        }
        
        return ans;
    }
}


//Game of XOR
